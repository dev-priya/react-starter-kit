{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React starter kit with React, styled-components, Webpack, Babel, Jest, Enzyme and ESLint configured out of the box for bootstrapping React projects with production ready toolchain.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.js\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "keywords": [
    "react",
    "styled-components",
    "starter-kit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-priya/react-starter-kit.git"
  },
  "author": "Dev Priya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dev-priya/react-starter-kit/issues"
  },
  "homepage": "https://github.com/dev-priya/react-starter-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1"
  }
}
